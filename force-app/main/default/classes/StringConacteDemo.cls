public class StringConacteDemo {
   public static string concateAndUpperCase(string a, string b){
string result= a + ', ' +b;
result = result.toUpperCase();
return result;
   }
   public static string concateLowerCase(string a, string b){
    string result= a + ', ' +b;
result = result.toLowerCase();
return result;
   }
   public static void printConcate(string a, string b){
string result = a +', ' +b;
system.debug(result);

   }
}
